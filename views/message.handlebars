
{{#if list}}
<br>
<div class="container jumbotron">
<h3 style="text-align: center; margin-bottom: 20px;">Reachouts</h3>
<div class="row">
    {{#each list}}
        <div class="col-md-4">
         <div class="card" style="width: 18rem; padding: 10px; margin-bottom: 15px;">
  <div class="card-body">
    <p class="card-text"> Name: {{ this.fname }}</p>
    <p class="card-text"> Email: {{ this.email }}</p>
    <p class="card-text"> Message: {{ this.message }}</p>
       </div>
   </div>
    </div>

    {{/each}}

  </div>
</div>

  {{else}}
  <div class="container jumbotron" style="margin-top:10px;">
      <h1>No Reachouts</h1>
  </div>
{{/if}}


